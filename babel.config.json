{
  "presets": [
    "@babel/preset-env", 
    "@babel/preset-typescript",
    [
      "@babel/preset-react", 
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "i18next-extract",
    "add-module-exports",
    "@babel/plugin-proposal-record-and-tuple",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-react-jsx-source",
    "@babel/plugin-transform-react-jsx-self"
  ]
}